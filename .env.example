# RevenueCat API Configuration
REVENUE_CAT_PUBLIC_KEY=your_public_key_here
REVENUE_CAT_SECRET_KEY=your_secret_key_here
REVENUE_CAT_WEBHOOK_SECRET=your_webhook_secret_here

# RevenueCat Webhook Configuration
REVENUE_CAT_WEBHOOK_URL=/revenue-cat/webhook

# RevenueCat API Version (v2)
REVENUE_CAT_API_VERSION=v2

# RevenueCat API Base URL (optional, defaults to https://api.revenuecat.com)
REVENUE_CAT_API_BASE_URL=https://api.revenuecat.com

# RevenueCat Webhook Configuration
REVENUE_CAT_WEBHOOK_ENABLED=true
REVENUE_CAT_WEBHOOK_QUEUE=default

# RevenueCat Cache Configuration (optional)
REVENUE_CAT_CACHE_TTL=3600 # Cache time to live in seconds
REVENUE_CAT_CACHE_ENABLED=true

# RevenueCat Logging Configuration (optional)
REVENUE_CAT_LOG_LEVEL=debug # debug, info, warning, error
REVENUE_CAT_LOG_ENABLED=true

# RevenueCat Error Handling (optional)
REVENUE_CAT_THROW_ON_ERROR=true # Whether to throw exceptions on API errors
REVENUE_CAT_RETRY_ON_ERROR=true # Whether to retry failed API calls
REVENUE_CAT_MAX_RETRIES=3 # Maximum number of retries for failed API calls 